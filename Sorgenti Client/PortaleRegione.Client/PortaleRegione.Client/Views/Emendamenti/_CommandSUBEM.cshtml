@model PortaleRegione.DTO.Domain.EmendamentiDto

@if (Model.AbilitaSUBEM && !Model.ATTI.BloccoEM)
{
    <div class="row">
        <div class="col s12 right-align">
            <a class="btn-flat pink-text"
               href="@Url.Action("NuovoSUBEmendamento", "Emendamenti", new {id = Model.UIDAtto, ref_em = Model.UIDEM})" title="Crea Subemendamento">
                <i class="material-icons right pink-text">add</i>
                CREA SUBEM
            </a>
        </div>
    </div>   
}