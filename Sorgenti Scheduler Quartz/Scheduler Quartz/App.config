        <conversionPattern value="[%d{dd/MM/yyyy}] - [%d{HH:mm:ss}] - [%t] %-5p %c - %m%n" />  
      </layout>  
    </appender>  
    <root>  
      <level value="All" />  
      <appender-ref ref="RollingFile" />  
    </root>  
  </log4net>
